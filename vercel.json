{
  "version": 2,
  "buildCommand": "bash build.sh",
  "builds": [
    {
      "src": "vetretire_project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "includeFiles": "db.sqlite3"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vetretire_project/wsgi.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}
